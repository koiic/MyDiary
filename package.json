{
  "name": "mydiary",
  "version": "1.0.0",
  "description": "An online diary for keeping records,event and information",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node --presets babel-preset-env ./index.js",
    "start": " babel-node --presets babel-preset-env ./index.js",
    "test": "cross-env NODE_ENV=test nyc mocha --require babel-register --timeout 60000 ./test/*.js --exit || true",
    "cover": "istanbul cover _mocha -- --require babel-register test",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
   
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koiic/MyDiary.git"
  },
  "keywords": [
    "Journal",
    "dairy"
  ],
  "author": "Ismail Ibrahim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/koiic/MyDiary/issues"
  },
  "homepage": "https://github.com/koiic/MyDiary#readme",
  "dependencies": {
    "babel-core": "^6.26.3",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "chai-things": "^0.2.0",
    "cross-env": "^5.2.0",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "expect": "^23.4.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.2",
    "nyc": "^12.0.2",
    "should": "^13.2.1",
    "volleyball": "^1.5.1"
  }
}
